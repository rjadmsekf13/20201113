	<%
		String id = "";
		Cookie[] cookies = request.getCookies();
	for (Cookie cook : cookies) {
		out.println("쿠키 명 : "+ cook.getName()+ "<br>");
		out.println("쿠키 값 : "+cook.getValue());
		out.println("<br/>");
		if(cook.getName().equals("id"))
			id= cook.getValue();
	}
	%>
	<form action="">
		id = <input type="text" value="<%=id%>">
	</form>
