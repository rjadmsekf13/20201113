 */
public class Member {
	String pname;
	String pgender;
	String pid;
	String ppw;
	String ppwcheck;
	String pwnth;
	String pcall;
	String pe_mail;
	String phobby;
	
	public Member(String pname, String pgender, String pid, String ppw, String ppwcheck, 
			String pwnth, String pcall, String pe_mail, String phobby)
	{
		super();
		this.pname = pname;
		this.pgender = pgender;
		this.pid = pid;
		this.ppw = ppw;
		this.ppwcheck = ppwcheck;
		this.pwnth = pwnth;
		this.pcall = pcall;
		this.pe_mail = pe_mail;
		this.phobby = phobby;
	}
}
