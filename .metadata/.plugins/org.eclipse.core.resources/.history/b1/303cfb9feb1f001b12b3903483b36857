package Tlqk.mem;

/**
 * @PackageName		: Tlqk.mem
 * @FileName		: Member.java
 * @Since			: 2020. 11. 6
 * @Author			: KB
 * @Description		: 
 * =====================================================================================
 * 								   Modification History
 * =====================================================================================
 * Date				Author				Note
 * -------------------------------------------------------------------------------------
 * 2020. 11. 6		KB				최초 작성
 *
 */
public class Member {
	String pname;
	String pgender;
	String pid;
	String ppw;
	String ppwcheck;
	String pwnth;
	String pcall;
	String pe_mail;
	String phobby;
	
	public Member(String pname, String pgender, String pid, String ppw, String ppwcheck, 
			String pwnth, String pcall, String pe_mail, String phobby)
	{
		super();
		this.pname = pname;
		this.pgender = pgender;
		this.pid = pid;
		this.ppw = ppw;
		this.ppwcheck = ppwcheck;
		this.pwnth = pwnth;
		this.pcall = pcall;
		this.pe_mail = pe_mail;
		this.phobby = phobby;
	}
	
	@Override
	public String toString()
	{
		return "Member [pname="+ pname+ ", pgender="+ pgender+ ", pid="+ pid+ ", ppw="+ ppw+ 
				", ppwcheck="+ ppwcheck+ ", pwnth="+ pwnth+ ", pcall="+ pcall+ ", pe_mail="+ 
				pe_mail+ ", phobby="+ phobby;
	}
	
	public Member()
	{
		
	}
	public String getPname() {
		return pname;
	}
	public void setPname(String pname) {
		this.pname = pname;
	}
	public String getPgender() {
		return pgender;
	}
	public void setPgender(String pgender) {
		this.pgender = pgender;
	}
	public String getPid() {
		return pid;
	}
	public void setPid(String pid) {
		this.pid = pid;
	}
	public String getPpw() {
		return ppw;
	}
	public void setPpw(String ppw) {
		this.ppw = ppw;
	}
	public String getPpwcheck() {
		return ppwcheck;
	}
	public void setPpwcheck(String ppwcheck) {
		this.ppwcheck = ppwcheck;
	}
	public String getPwnth() {
		return pwnth;
	}
	public void setPwnth(String pwnth) {
		this.pwnth = pwnth;
	}
	public String getPcall() {
		return pcall;
	}
	public void setPcall(String pname) {
		this.pcall = pcall;
	}
	public String getPe_mail() {
		return pe_mail;
	}
	public void setPe_mail(String pe_mail) {
		this.pe_mail = pe_mail;
	}
	public String getPhobby() {
		return phobby;
	}
	public void setPhobby(String phobby) {
		this.phobby = phobby;
	}
}
